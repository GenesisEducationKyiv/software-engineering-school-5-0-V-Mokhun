{
  "name": "notifications",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "dotenv -e ../../.env.development -- nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test jest --config jest.unit.config.js --silent --runInBand",
    "db:generate": "npx prisma generate"
  },
  "prisma": {
    "seed": "ts-node ../../prisma/seed.ts",
    "schema": "../../prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.5.0",
    "bullmq": "^5.52.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "morgan": "^1.10.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "6.5.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  }
}
