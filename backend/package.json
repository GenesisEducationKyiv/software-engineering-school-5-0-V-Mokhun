{
  "name": "backend",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:arch": "npm run test:arch --workspaces --if-present",
    "test:unit": "npm run test:unit --workspaces --if-present",
    "test:integration": "npm run test:integration --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "build": "tspc --build",
    "type-check": "npm run type-check --workspaces --if-present",
    "docker:run": "docker compose -f docker-compose.yml up -d",
    "docker:build": "docker compose -f docker-compose.yml build",
    "docker:run:dev": "docker compose -f docker-compose.dev.yml up -d",
    "docker:build:dev": "docker compose -f docker-compose.dev.yml build",
    "db:generate": "npm run db:generate --workspaces --if-present",
    "db:migrate": "npm run db:migrate --workspaces --if-present",
    "db:migrate-local": "npm run db:migrate-local --workspaces --if-present",
    "db:migrate-local:dev": "npm run db:migrate-local:dev --workspaces --if-present",
    "db:migrate-test": "npm run db:migrate-test --workspaces --if-present",
    "db:reset": "npm run db:reset --workspaces --if-present",
    "db:reset-local": "npm run db:reset-local --workspaces --if-present",
    "db:reset-local:dev": "npm run db:reset-local:dev --workspaces --if-present",
    "db:seed": "npm run db:seed --workspaces --if-present",
    "proto:generate": "npx buf generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "bullmq": "^5.52.2",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "ioredis": "^5.6.1",
    "opossum": "^9.0.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.34.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@eslint/js": "^9.26.0",
    "@types/opossum": "^8.1.9",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.26.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "typescript-transform-paths": "^3.5.5"
  }
}
